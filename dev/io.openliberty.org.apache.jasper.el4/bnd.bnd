-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Open Liberty EL 4.0
Bundle-SymbolicName: io.openliberty.org.apache.jasper.el4
Bundle-Description: Open Liberty Expression Language 4.0 Implementation, Version=${bVersion}
Implementation-Version: 4.0.0

Export-Package: org.apache.el;thread-context=true;version=${Implementation-Version}, \
   org.apache.el.lang;thread-context=true;version=${Implementation-Version}, \
   org.apache.el.parser;version=${Implementation-Version}, \
   org.apache.el.util;version=${Implementation-Version}, \
   org.apache.el.stream;version=${Implementation-Version}, \

Private-Package: org.apache.el.*

Import-Package: \
    jakarta.el; version=4.0.0, \
    *

instrument.ffdc: false

instrument.classesExcludes: org/apache/el/Message*.class

-buildpath: \
    org.apache.tomcat:tomcat-jasper-el;version=latest,\
	com.ibm.websphere.jakartaee.el.4.0;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
