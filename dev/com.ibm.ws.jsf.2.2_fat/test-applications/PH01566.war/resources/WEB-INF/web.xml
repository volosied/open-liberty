	<servlet>
		<servlet-name>DoesNotExist</servlet-name>
		<servlet-class>com.ibm.ws.DoesNotExist</servlet-class>
	</servlet>

  <!-- Welcome page -->
  <welcome-file-list>
    <welcome-file>index.xhtml</welcome-file>
  </welcome-file-list>
    
</web-app>