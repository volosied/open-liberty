-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: io.openliberty.com.sun.el4
Bundle-Description: Jakarta Expression Language EE9 Implementation, version 4.0
Implementation-Version: 4.0.0

Export-Package: com.sun.el;thread-context=true;version=${Implementation-Version}, \
   com.sun.el.lang;thread-context=true;version=${Implementation-Version}, \
   com.sun.el.parser;version=${Implementation-Version}, \
   com.sun.el.util;version=${Implementation-Version}, \
   com.sun.el.stream;version=${Implementation-Version}, \

Import-Package: \
    jakarta.el; version=4.0.0, \
    *

instrument.ffdc: false

-buildpath: \
    org.glassfish:jakarta.el;version=4.0.0
