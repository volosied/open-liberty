-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0
-nouses=true

Bundle-SymbolicName: io.openliberty.jakarta.pages.tld.3.1
Bundle-Description: Jakarta Standard Tag Library, version 3.0

Import-Package: \
   !com.oracle.wls.shaded.org.apache.xml.serialize,\
   !com.oracle.wls.shaded.org.apache.xml.serializer,\
   !org.apache.xerces*, \
   *

Export-Package: \
    org.apache.taglibs.standard.tag.common.core;version="3.0.0", \
    org.apache.taglibs.standard.tag.common.xml;version="3.0.0", \
    org.apache.taglibs.standard.tag.el.core;version="3.0.0", \
    org.apache.taglibs.standard.tag.el.fmt;version="3.0.0", \
    org.apache.taglibs.standard.tag.el.sql;version="3.0.0", \
    org.apache.taglibs.standard.tag.el.xml;version="3.0.0", \
    org.apache.taglibs.standard.tag.rt.core;version="3.0.0", \
    org.apache.taglibs.standard.tag.rt.fmt;version="3.0.0", \
    org.apache.taglibs.standard.tag.rt.sql;version="3.0.0", \
    org.apache.taglibs.standard.tag.rt.xml;version="3.0.0"

-includeresource: @${repo;org.glassfish.web:jakarta.servlet.jsp.jstl;3.0.0;EXACT}!/!org/eclipse/tags/shaded/*

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec
